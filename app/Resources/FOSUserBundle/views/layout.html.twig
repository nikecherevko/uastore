<!DOCTYPE html>
<html lang="ru">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
        <title>{% block title %}Login page{% endblock %}</title>
        <link href="{{ asset('css/bootstrap.min.css') }}" rel="stylesheet" />
        <link href="{{ asset('bundles/adminuser/css/admin_category.css') }}" rel="stylesheet" />
        <link href="{{ asset('bundles/adminuser/css/fosuser.css') }}" rel="stylesheet" />
        <link href="{{ asset('css/form.css') }}" rel="stylesheet" />
        
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body class="register-page">
        
    <div class="register-box">
        <div class="register-logo">
            <a href="#">
            <b>Admin</b>
            STORE
            </a>
        </div>
        <div class="register-box-body">
            {% block fos_user_title %} {% endblock fos_user_title %}
            
            {% block fos_user_content %} {% endblock fos_user_content %}
        </div>    
    </div>    
    {% block javascripts %} 
        <script type="text/javascript" src="{{ asset('js/jquery-2.1.3.min.js') }}"></script>
    {% endblock %}   
    </body>
</html>