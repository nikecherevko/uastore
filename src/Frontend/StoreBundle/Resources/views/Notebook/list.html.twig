                        <div class="clr"></div>
                    </div>
                    <div class="price_blockz">
                        <div class="price">
                            <div class="priced fdf">
                                <span class="main">
                                    {{entity.price|number_format(0, '.', ' ')}}
                                    <span class="currency"> грн.</span>
                                </span>
                            </div>
                            <div class="buy_button">
                                <button class="btn btn-danger btn-xx" type="button" onclick="doCart({{entity.id}});">Купить</button>
                                <a href="{{ path('notebook_show', {'id': entity.id, 'slug':slug}) }}">
                                    <button class="btn btn-default btn-xa" type="button">Подробности</button>
                                </a>
                                <div class="clr"></div>
                            </div>      
                        </div>      
                    </div>      
                    <div class="block_title">Особенности</div>
                    <div class="descr">
                        <div class="info">
                            <ul class="sv">
                                <li>
                                    <span>Проц.:</span>
                                    {{entity.cpuSeries}}, {{entity.cpuFrequency}}
                                </li>
                                <li>
                                    <span>Экран:</span>
                                    {{entity.display}}, {{entity.displayResolution}}
                                </li>
                                <li>
                                    <span>Видео:</span>
                                    {{entity.videoadapterManufacturer}}, {{entity.videoadapter}}
                                </li>
                                <li>
                                    <span>ОЗУ:</span>
                                     {{entity.ramType}}, {{entity.ramSize}}
                                </li>
                                <li>
                                    <span>ПЗУ:</span>
                                     {{entity.hddType}}, {{entity.hddSize}}
                                </li>
                                <li>
                                    <span>Батарея:</span>
                                    {{entity.batteries}}
                                </li>
                                <li>
                                    <span>ОС:</span>
                                    {{entity.os}}
                                </li>
                                <li>
                                    <span>Цвет корпуса:</span>
                                    {{entity.color}}
                                </li>
                                <li>
                                    <span>Вес:</span>
                                    {{entity.weight}}
                                </li>
                            </ul>
                        </div>
                        <div class="clr"></div>
                    </div>
                </div>
            </div>
        </div>    
        <div class="clr"></div>
    </div>   
</div>                              
