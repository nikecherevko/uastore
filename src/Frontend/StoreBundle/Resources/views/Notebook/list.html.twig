{% if entitiesf %}
    
<div class="accepted">
    <ul>
        {% for entityf in entitiesf %}
        <li>
           {{entityf.value}}<span onclick="delfitlr({{entityf.id}})"></span>
        </li>
       {% endfor %}
        <li class="closeli">
            Очистить фильтры <span  class="close"  onclick="delfitlr(0)" title="очистить фильтры"></span>
        </li> 
    </ul>
    <div class="clr"></div>
</div>   
{% endif %} 

{% if entities %}
{% for entity in entities %}

<div class="catalog_product_list view_list">
    <div id="item-{{entity.id}}" >
        {% set slug = entity.manufacturer ~ entity.model |slugify %} 
        <div class="imgindex">
            <a href="{{ path('notebook_show', {'id': entity.id, 'slug': slug}) }}">
                <img  src="{{ asset('images/notebook/'~entity.image) | apply_filter('widthlogo300') }}"   alt="logo" />
            </a>
             
        </div>    
        <div class="infoindex">
            <div class="info_block">
                <div class="additional_info">
                    <div class="titleindex">
                        <a href="{{ path('notebook_show', {'id': entity.id, 'slug': slug}) }}">Ноутбук {{entity.manufacturer}} {{entity.model}} ({{entity.modification}})</a>
                    </div> 
                    <br>
                    <div class="params_icons_block">
                        {% if 'Intel Core i7' in entity.cpuSeries  %}   
                        <div class="params_icons_block_item">
                            <div class="params_icons_block_img">
                            <img  width="48" height="48" alt="Intel Core i7" src="{{ asset('images/picons/intel_i7.png') }}">
                            </div>
                            <div class="params_icons_block_title">Intel Core i7</div>
                        </div> 
                        {% endif %}
                        
                        {% if 'Intel Core i5' in entity.cpuSeries  %}   
